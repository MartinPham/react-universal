{
  "name": "react-craco-universal",
  "version": "0.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "e-start": "expo start",
    "e-android": "expo start --android",
    "e-ios": "expo start --ios",
    "e-clean": "watchman watch-del-all && rm -rf $TMPDIR/metro-* && rm -rf node_modules/.cache",
    "e-eject": "expo eject",
    "rs-start": "react-scripts start",
    "rs-build": "react-scripts build",
    "rs-test": "react-scripts test",
    "rs-eject": "react-scripts eject",
    "start": "cross-env NODE_PATH=src craco start",
    "build": "touch .building && rm -rf build && cross-env NODE_PATH=src craco build && rm .building",
    "test": "cross-env NODE_PATH=src craco test",
    "build:server": "touch .building && rm -rf build && rm -rf build-server && cross-env NODE_PATH=src node scripts/buildServer && mv build build-server && rm .building",
    "build:client": "touch .building && rm -rf build && cross-env NODE_PATH=src node scripts/buildClient && rm .building",
    "build:all": "touch .building-all && npm run build:server && npm run build && rm .building-all",
    "start:server": "cross-env NODE_PATH=src node scripts/startServer 3000"
  },
  "dependencies": {
    "expo": "^31.0.2",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-frontload": "^1.0.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-native": "^0.57.7",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@craco/craco": "^3.1.1",
    "babel-preset-expo": "^5.0.0",
    "chokidar": "^2.0.4",
    "clear-module": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "find-free-port": "^2.0.0",
    "node-sass": "^4.10.0",
    "npm-run": "^5.0.1",
    "webpack-node-externals": "^1.7.2",
    "ws": "^6.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "private": true
}
