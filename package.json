{
	"name": "react-universal",
	"version": "0.1.0",
	"homepage": "https://martinpham.gitlab.io/react-universal",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"native:start": "expo start",
		"native:android": "expo start --android",
		"native:ios": "expo start --ios",
		"native:clean": "watchman watch-del-all && rm -rf $TMPDIR/metro-* && rm -rf node_modules/.cache",
		"expo:eject": "expo eject",
		"cra:start": "react-scripts start",
		"cra:build": "react-scripts build",
		"cra:test": "react-scripts test",
		"cra:eject": "react-scripts eject",
		"start": "npm run browser:dev",
		"browser:dev": "cross-env NODE_PATH=src craco start",
		"browser:build": "touch .building && rm -rf build && rm -rf build-browser && cross-env NODE_PATH=src craco build && mv build build-browser && rm .building",
		"browser:test": "cross-env NODE_PATH=src craco test",
		"browser:prod:run": "node scripts/browser/prod 3000",
		"browser:prod": "npm run browser:build && npm run browser:prod:run",
		"browser:deploy:github": "npm run browser:build && gh-pages -d build-browser",
		"server:build": "touch .building && rm -rf build && rm -rf build-server && cross-env NODE_PATH=src node scripts/server/build && mv build build-server && rm .building",
		"server:build:all": "touch .building-all && npm run server:build && npm run browser:build && rm .building-all",
		"server:prod:run": "node scripts/server/prod 3000",
		"server:prod": "npm run server:build:all && npm run server:prod:run",
		"cli:dev": "cross-env PLATFORM=cli node scripts/cli/dev",
		"cli:build": "touch .building && rm -rf build && rm -rf build-cli && cross-env PLATFORM=cli node scripts/cli/build && mkdir build-cli && mv build/index.js build-cli/index.js && node scripts/cli/build-assets && rm .building",
		"cli:prod:run": "node build-cli",
		"cli:prod": "npm run cli:build && npm run cli:prod:run"
	},
	"dependencies": {
		"history": "^4.7.2",
		"immutable": "^4.0.0-rc.12",
		"ink": "^0.5.1",
		"ink-redux": "^1.0.0",
		"query-string": "5",
		"react": "^16.6.3",
		"react-dom": "^16.6.3",
		"react-frontload": "^1.0.3",
		"react-helmet": "^5.2.0",
		"react-loadable": "^5.5.0",
		"react-native": "^0.57.7",
		"react-redux": "^5.1.1",
		"react-router": "^4.3.1",
		"react-spring": "^7.0.0",
		"react-transition-group": "^2.5.0",
		"redux": "^4.0.1",
		"redux-saga": "^0.16.2",
		"reselect": "^4.0.0",
		"styled-components": "^4.1.2"
	},
	"devDependencies": {
		"@craco/craco": "^3.1.1",
		"@material-ui/core": "^3.6.2",
		"babel-preset-expo": "^5.0.0",
		"chokidar": "^2.0.4",
		"clear-module": "^3.0.0",
		"cookie-parser": "^1.4.3",
		"cross-env": "^5.2.0",
		"expo": "^31.0.2",
		"find-free-port": "^2.0.0",
		"gh-pages": "^2.0.1",
		"ink-big-text": "^0.3.0",
		"ink-box": "^0.1.0",
		"ink-gradient": "^0.1.1",
		"ink-select-input": "^2.0.0",
		"ink-spinner": "^2.0.0",
		"ink-text-animation": "^0.1.0",
		"ink-text-input": "^2.0.0",
		"node-sass": "^4.10.0",
		"npm-run": "^5.0.1",
		"qrcode-terminal": "^0.12.0",
		"react-icons": "^3.2.2",
		"react-scripts": "2.1.1",
		"reactotron-react-js": "^2.1.3",
		"reactotron-react-native": "^2.1.3",
		"terminal-image": "^0.1.2",
		"webpack-node-externals": "^1.7.2",
		"ws": "^6.1.2"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"private": true
}
